{
  "name": "defendao",
  "version": "1.0.0",
  "description": "DefenDAO Monorepo",
  "scripts": {
    "build:server": "turbo run build --filter=@defendao/server",
    "dev:server": "turbo run dev --filter=@defendao/server",
    "install:dashboard": "cd ./apps/dashboard && npm install",
    "install:indexer": "cd ./apps/indexer && npm install",
    "install:server": "cd ./apps/server && npm install",
    "start:server":"turbo run start:prod --filter=@defendao/server",
    "start:indexer": "turbo run start --filter=@defendao/indexer"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "turbo": "^2.3.1",
    "@defendao/indexer": "./apps/indexer",
    "@defendao/server": "./apps/server"
  },
  "packageManager": "pnpm@9.14.2"
}
