{
  "name": "@anticapture/indexer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "ponder dev --schema public",
    "start": "ponder start --schema public",
    "start-raw": "npm run drop-public && ponder start --schema public",
    "codegen": "ponder codegen",
    "serve": "npm run setup-drizzle && ponder serve --schema public",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "typecheck": "tsc",
    "test": "jest --detectOpenHandles",
    "clear-database": "tsx ./test/lib/database/clearDatabase.script.ts",
    "locally:deploy-ens": "cd ./contracts && npm run locally:deploy-ens",
    "prepare": "husky",
    "dump-database": "rm -rf dump && pg_dump $DATABASE_URL -n ponder_sync > dump",
    "restore-database": "psql -d $DATABASE_URL < dump",
    "drop-public": "psql -d $DATABASE_URL -c 'DROP SCHEMA public CASCADE;'",
    "drop-all-schemas": "psql -d $DATABASE_URL -c \"DROP SCHEMA IF EXISTS public CASCADE; DROP SCHEMA IF EXISTS ponder_sync CASCADE\"",
    "setup-drizzle": "drizzle-kit migrate"
  },
  "dependencies": {
    "@hono/swagger-ui": "^0.5.0",
    "@viem/anvil": "^0.0.10",
    "drizzle-kit": "^0.30.4",
    "drizzle-orm": "^0.39.3",
    "hono": "^4.5.0",
    "ioredis": "^5.5.0",
    "ponder": "^0.9.19",
    "viem": "^2.21.9"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.4",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.16.5",
    "@types/pg": "^8.11.10",
    "axios": "^1.7.7",
    "babel-jest": "^29.7.0",
    "dotenv": "^16.4.7",
    "eslint": "^8.53.0",
    "eslint-config-ponder": "^0.5.6",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-config": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.14"
  }
}
